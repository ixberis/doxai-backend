        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "100 * ( sum(increase(payments_webhooks_processed_total{provider=~\"$provider\",success=\"false\"}[$__interval])) ) / clamp_min(sum(increase(payments_webhooks_processed_total{provider=~\"$provider\"}[$__interval])),1)",
